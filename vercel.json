{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "POST",
        "GET",
        "PUT",
        "PATCH",
        "DELETE"
      ]
    }
  ],
  "env": {
    "TS_NODE_TRANSPILE_ONLY": "true",
    "TS_NODE_PROJECT": "tsconfig.build.json"
  }
}