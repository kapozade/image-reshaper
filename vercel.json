{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "POST",
        "GET",
        "PUT",
        "PATCH",
        "DELETE"
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/swagger/(.*)",
      "destination": "/public/swagger/$1"
    },
    {
      "source": "/swagger.json",
      "destination": "/public/swagger.json"
    }
  ],
  "env": {
    "TS_NODE_TRANSPILE_ONLY": "true",
    "TS_NODE_PROJECT": "tsconfig.build.json",
    "NODE_ENV": "production"
  }
}